{
  "manifest_version": 2,
  "name": "Craigslist Spam Buttons",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["clspambuttons.css"],
      "js": ["jquery-1.9.1.js", "clspambuttons.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["flagAsSpam.js","jquery.min.map"],
  "description": "Adds links to flag ads as spam directly from the listing page."
}